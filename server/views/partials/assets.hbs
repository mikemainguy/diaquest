<a-assets timeout="2000">
    <a-mixin id="menuPlane"
             material="color: #22a"
             width="1"
             height="1"
             text="align: center; wrapCount: 10"
             position="0 .3 .05 ">
    </a-mixin>
    <a-mixin id="widgetMaterial"
             material="color: #aaa; opacity: 0.5">
    </a-mixin>
    <a-mixin id="sizeConnectorMixin"
             class="sizeHandle"
             material="color: #ff0; opacity: 0.5">
    </a-mixin>
    <a-mixin id="labelMixin"
             material="color: #eee;"
             lookatme="frequency: 220"
             text="height: .4; width: 2; align: center; value: ...;"
             ></a-mixin>


    <script id="animationmanager-template" type="html">
        {{> animationMenu }}
    </script>
    <audio id="audiohover" src="/assets/sounds/KeyInLow.mp3" preload="auto"></audio>
    <audio id="audioclick" src="/assets/sounds/KeyInLow.mp3" preload="auto"></audio>
    <script id="plus" type="html">
        <a-entity position="0 0 .1">
            <a-box
                    width="0.05"
                    height=".2"
                    depth="0.05"
                    color="#5f5">

            </a-box>
            <a-box
                    width=".2"
                    height="0.05"
                    depth="0.05"
                    color="#5f5">

            </a-box>
        </a-entity>
    </script>

    <script id="widgetPoint" type="html">
        <a-cone
                radius-bottom="0.03"
                radius-top="0"
                height="0.1"
                mixin="widgetMaterial">
        </a-cone>
    </script>

    <script id="connector-template" type="html">
        <a-entity class="data-connector" share-position>
            <a-cone rotation="-90 0 0" segments-height="1" segments-radial="8" position="0 0 0" radius-top="0" radius-bottom="0.02" height=".04" class="data-packet" color="#111">
            </a-cone>
            <a-cylinder segments-height="1" segments-radial="8" open-ended="true" class="saveable data-direction" rotation="90 0 0" position="0 0 0"
                        material="opacity: 0.9; color: #33f" height="1" radius="0.06" visible="false">
            </a-cylinder>
            <a-entity class="label" mixin="labelMixin" position="0 0.3 0"></a-entity>
        </a-entity>
    </script>
    <script id="jiratemplate" type="html">
        <a-entity class="key" position="0 0 .0501" text="value: ???; color: #222; align: center">
        </a-entity>
        <a-plane class="summary" width=".8" height="0.1" position="0 .105 .05" lookatme text="value: test; align: center" color="#222">
        </a-plane>
        <a-plane class="description" position="-.6 0 0"
            width="1"
            height="1"
            visible="false">
        </a-plane>
        <a-box color="#333" class="saveable" width=".1" height=".1" depth=".1">
        </a-box>
    </script>
    <script id="sequenceTemplate" type="html">
        <a-box class="base" color="#333" width="5" height=".1" depth="5">
        </a-box>
        <a-plane width="0.8" height="0.1" color="#333" position='0 0 5.01' text="align: center" class='text'></a-plane>
    </script>
    <script id="jirastatusTemplate" type="html">
        <a-box color="#229" width=".8" height=".1" depth="10">
        </a-box>
        <a-plane width="0.8" height="0.1" color="#333" position='0 0 5.01' text="align: center" class='text'></a-plane>
    </script>
    <script id="light-template" type="html">
            <a-cylinder
                    share-position
                    radius=".01"
                    height=".1"
                    class="saveable"
                    visible="false">
            </a-cylinder>
            <a-entity
                    rotation="90 0 0"
                    light="type: spot; angle: 10; distance: 40">
            </a-entity>
            <a-entity
                    mixin="labelMixin"
                    position="0 0.3 0">
            </a-entity>

    </script>

    <script
            id="sphere-template"
            type="html">
        <a-entity share-position>
            <a-sphere
                    class="saveable"
                    material="color: #33f"
                    segments-height="12"
                    segments-width="24"
                    radius=".5"
                    visible="false">
            </a-sphere>
            <a-entity
                    mixin="labelMixin"
                    position="0 0.3 0">
            </a-entity>
        </a-entity>
    </script>

    <script
            id="tetrahedron-template"
            type="html">
        <a-entity share-position>
            <a-tetrahedron
                    class="saveable"
                    material="color: #33f"
                    radius=".5"
                    visible="false">
            </a-tetrahedron>
            <a-entity
                    mixin="labelMixin"
                    position="0 0.3 0">
            </a-entity>
        </a-entity>
    </script>

    <script
            id="tetrahedron-template2"
            type="html">
        <a-entity share-position>
            <a-octahedron
                    class="saveable"
                    material="color: #33f"
                    radius=".5"
                    visible="false">
            </a-octahedron>
            <a-entity
                    mixin="labelMixin"
                    position="0 0.3 0">
            </a-entity>
        </a-entity>
    </script>

    <script
            id="cylinder-template"
            type="html">
        <a-entity share-position>
            <a-cylinder
                    class="saveable"
                    material="color: #33f"
                    radius=".5"
                    segments-height="1"
                    height="1"
                    visible="false">
            </a-cylinder>
            <a-entity mixin="labelMixin" position="0 0.3 0"></a-entity>
        </a-entity>
    </script>
    <script id="box-template" type="html">
        <a-entity grabbable share-position>
            <a-box

                    class="saveable"
                    color="#00f"
                    width="1"
                    height="1"
                    depth="1"
                    visible="false">

            </a-box>
            <a-entity mixin="labelMixin" position="0 0.3 0"></a-entity>
        </a-entity>
    </script>
    <script id="plane-template" type="html">
        <a-entity grabbable share-position>
            <a-plane
                    material="side: double"
                    class="saveable"
                    color="#fff"
                    width="1"
                    height="1"
                    visible="false">

            </a-box>
            <a-entity mixin="labelMixin" position="0 0.3 0"></a-entity>
        </a-entity>
    </script>
    <script id="user-template" type="html">
        <a-entity avatar share-position>

            <a-entity rotation="0 0 0" class="head" scale="0.2 0.2 0.15">
                <a-sphere
                    position="0 .5 0"
                    material="color: #ff0; opacity: 1"
                    radius=".5">
                    </a-sphere>
                <a-entity>
                <a-sphere position=".2 .6 -.4"
                    radius="0.1" color="#33c">
                </a-sphere>
                <a-sphere position="-.2 .6 -.4"
                    radius="0.1" color="#33c">
                </a-sphere>
            </a-entity>
            </a-entity>
            <a-entity
                    mixin="labelMixin"
                    position="0 .5 0">
            </a-entity>
            <a-cone class="body" position="0 -.5 0" radius-bottom=".2" radius-top=".04" height="1" color="#ff0">

            </a-box>
        </a-entity>
    </script>
    <img id="voiceicon" src="/assets/voice.svg">
    <!--<img id="linkImage" src="/assets/castle.jpg">-->

</a-assets>