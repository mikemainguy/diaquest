<a-assets timeout="2000">
<!--    <a-entity id="testbutton"
              rotation="90 45 0" position="0 1.7 -2" scale=".2 .2 .2" material="color: #f00" obj-model="obj: url(/assets/models/button.obj)"></a-entity>
              -->
    <a-mixin id="menuPlane"
             material="color: #22a"
             width="1"
             height="1"
             text="align: center; wrapCount: 10"
             position="0 .3 .05 ">
    </a-mixin>
    <a-mixin id="widgetMaterial"
             material="color: #aaa; opacity: 0.5">
    </a-mixin>
    <a-mixin id="sizeConnectorMixin"
             class="sizeHandle"
             material="color: #ff0; opacity: 0.5">
    </a-mixin>
    <a-mixin id="labelMixin"
             material="color: #eee;"
             lookatme
             text="height: .4; width: 2; align: center; value: ...;"
             ></a-mixin>


    <script id="animationmanager-template" type="html">
        {{> animationMenu }}
    </script>

    <script id="plus" type="html">
        <a-entity position="0 0 .1">
            <a-box
                    width="0.05"
                    height=".2"
                    depth="0.05"
                    color="#5f5">

            </a-box>
            <a-box
                    width=".2"
                    height="0.05"
                    depth="0.05"
                    color="#5f5">

            </a-box>
        </a-entity>
    </script>

    <script id="widgetPoint" type="html">
        <a-cone
                radius-bottom="0.03"
                radius-top="0"
                height="0.1"
                mixin="widgetMaterial">
        </a-cone>
    </script>

    <script id="connector-template" type="html">
        <a-entity class="data-connector" share-position>
            <a-cone rotation="-90 0 0" segments-height="1" segments-radial="16" position="0 0 0" radius-top="0" radius-bottom="0.02" height=".04" class="data-packet" color="#111">
            </a-cone>
            <a-cylinder segments-height="1" segments-radial="16" open-ended="true" class="saveable data-direction" rotation="90 0 0" position="0 0 0"
                        material="opacity: 0.9; color: #33f" height="1" radius="0.06" visible="false">
            </a-cylinder>
            <a-entity class="label" mixin="labelMixin" position="0 0.3 0"></a-entity>
        </a-entity>
    </script>
    <script id="light-template" type="html">
            <a-cylinder
                    share-position
                    radius=".01"
                    height=".1"
                    class="saveable"
                    visible="false">
            </a-cylinder>
            <a-entity
                    rotation="90 0 0"
                    light="type: spot; angle: 10; distance: 40">
            </a-entity>
            <a-entity
                    mixin="labelMixin"
                    position="0 0.3 0">
            </a-entity>

    </script>

    <script
            id="sphere-template"
            type="html">
        <a-entity share-position>
            <a-sphere
                    class="saveable"
                    material="color: #33f"
                    segments-height="16"
                    radius=".5"
                    visible="false">
            </a-sphere>
            <a-entity
                    mixin="labelMixin"
                    position="0 0.3 0">
            </a-entity>
        </a-entity>
    </script>

    <script
            id="tetrahedron-template"
            type="html">
        <a-entity share-position>
            <a-tetrahedron
                    class="saveable"
                    material="color: #33f"
                    radius=".5"
                    visible="false">
            </a-tetrahedron>
            <a-entity
                    mixin="labelMixin"
                    position="0 0.3 0">
            </a-entity>
        </a-entity>
    </script>

    <script
            id="tetrahedron-template2"
            type="html">
        <a-entity share-position>
            <a-octahedron
                    class="saveable"
                    material="color: #33f"
                    radius=".5"
                    visible="false">
            </a-octahedron>
            <a-entity
                    mixin="labelMixin"
                    position="0 0.3 0">
            </a-entity>
        </a-entity>
    </script>

    <script
            id="cylinder-template"
            type="html">
        <a-entity share-position>
            <a-cylinder
                    class="saveable"
                    material="color: #33f"
                    radius=".5"
                    segments-height="1"
                    height="1"
                    visible="false">
            </a-cylinder>
            <a-entity mixin="labelMixin" position="0 0.3 0"></a-entity>
        </a-entity>
    </script>
    <script id="box-template" type="html">
        <a-entity grabbable share-position>
            <a-box

                    class="saveable"
                    color="#00f"
                    width="1"
                    height="1"
                    depth="1"
                    visible="false">

            </a-box>
            <a-entity mixin="labelMixin" position="0 0.3 0"></a-entity>
        </a-entity>
    </script>
    <script id="plane-template" type="html">
        <a-entity grabbable share-position>
            <a-plane
                    material="side: double"
                    class="saveable"
                    color="#fff"
                    width="1"
                    height="1"
                    visible="false">

            </a-box>
            <a-entity mixin="labelMixin" position="0 0.3 0"></a-entity>
        </a-entity>
    </script>
    <script id="user-template" type="html">
        <a-entity share-position>
        <a-entity rotation="0 0 0">
            <a-entity rotation="0 0 0" scale="0.2 0.2 0.1">
                <a-sphere
                position="0 .5 0"
                material="color: #ff0; opacity: .9"
                radius=".5">
                </a-sphere>
            <a-entity>
            <a-sphere position=".2 .6 -.4"
                radius="0.1" color="#33c">
            </a-sphere>
            <a-sphere position="-.2 .6 -.4"
                radius="0.1" color="#33c">
            </a-sphere>
            </a-entity>
            <a-entity
                    mixin="labelMixin"
                    position="0 1 0">
            </a-entity>
        </a-entity>
    </script>
    <img id="voiceicon" src="/assets/voice.svg">
    <img id="linkImage" src="/assets/castle.jpg">

</a-assets>