<a-entity position="0 .15 -.2" rotation="0 -45 0" id="bmenu" visible="false" scale="0.1 0.1 0.1">
    <a-entity sound="src: url(/assets/sounds/KeyIn.mp3); on: click; poolSize: 5" gridlayout position="0 -.5 0">
        <a-entity>
            <a-entity text="value: Sphere"  mixin="menuPlane"></a-entity>
            <a-entity widget="method: add-sphere"
                      obj-model="obj: url(/assets/models/button.obj)"
                      material="color: #fce; opacity: .6"
                      scale=".5 .5 .5" rotation="90 0 0"></a-entity>

            <a-sphere radius="0.125"
                      mixin="widgetMaterial">
                <a-entity template="src: #plus"></a-entity>

            </a-sphere>

        </a-entity>
        <a-entity>
            <a-entity text="value: Box"  mixin="menuPlane"></a-entity>
            <a-entity widget="method: add-box"
                      obj-model="obj: url(/assets/models/button.obj)"
                      material="color: #fce; opacity: .6"
                      scale=".5 .5 .5" rotation="90 0 0"></a-entity>

            <a-box
                    width="0.25"
                    height="0.25"
                    depth="0.25"
                    mixin="widgetMaterial">
                <a-entity template="src: #plus"></a-entity>
            </a-box>

        </a-entity>
        <a-entity>
            <a-entity text="value: Cylinder"  mixin="menuPlane"></a-entity>
            <a-entity widget="method: add-cylinder"
                      obj-model="obj: url(/assets/models/button.obj)"
                      material="color: #fce; opacity: .6"
                      scale=".5 .5 .5" rotation="90 0 0"></a-entity>

            <a-cylinder radius="0.125" height=".25"
                        mixin="widgetMaterial">
                <a-entity template="src: #plus"></a-entity>
            </a-cylinder>

        </a-entity>
        <a-entity>
            <a-entity>
                <a-entity text="value: Connector"  mixin="menuPlane"></a-entity>
                <a-entity widget="method: add-connector"
                          obj-model="obj: url(/assets/models/button.obj)"
                          material="color: #fce; opacity: .6"
                          scale=".5 .5 .5" rotation="90 0 0"></a-entity>

                <a-entity template="src: #widgetPoint" position="0 0.2 0"></a-entity>
                <a-cylinder radius=".01" height="0.3" color="#fff"></a-cylinder>
                <a-entity template="src: #plus"></a-entity>

            </a-entity>
        </a-entity>
        <a-entity>
            <a-entity
                      text="value: Text"  mixin="menuPlane"></a-entity>
            <a-entity widget="method: edit" obj-model="obj: url(/assets/models/button.obj)" material="color: #fce; opacity: .6" scale=".5 .5 .5" rotation="90 0 0"></a-entity>
            <a-entity>
                <a-box width=".05" height="0.05" depth="0.05" mixin="widgetMaterial"></a-box>
                <a-box width=".05" position="-.1 0 0" height="0.05" depth="0.05" mixin="widgetMaterial"></a-box>
                <a-box width=".05" position=".1 0 0" height="0.05" depth="0.05" mixin="widgetMaterial"></a-box>
            </a-entity>
        </a-entity>
        <a-entity>
            <a-entity text="value: Edit Size"  mixin="menuPlane"></a-entity>
            <a-entity widget="method: resize" obj-model="obj: url(/assets/models/button.obj)" material="color: #fce; opacity: .6" scale=".5 .5 .5" rotation="90 0 0"></a-entity>
            <a-entity>
                <a-entity rotation="30 30 0">
                    <a-cylinder radius=".01" height="0.3" mixin="widgetMaterial"></a-cylinder>
                    <a-entity template="src: #widgetPoint" position="0 .2 0"></a-entity>
                    <a-entity template="src: #widgetPoint" rotation="0 0 180" position="0 -.2 0"></a-entity>

                </a-entity>
                <a-entity rotation="30 30 90">
                    <a-cylinder radius=".01" height="0.3" mixin="widgetMaterial"></a-cylinder>
                    <a-entity template="src: #widgetPoint" position="0 .2 0" opacity="0.5"></a-entity>
                    <a-entity template="src: #widgetPoint" rotation="0 0 180"
                              position="0 -.2 0"></a-entity>
                </a-entity>
                <a-entity rotation="120 30 0">
                    <a-cylinder radius=".01" height="0.3" color="#fff"></a-cylinder>
                    <a-entity template="src: #widgetPoint" position="0 .2 0"></a-entity>
                    <a-entity template="src: #widgetPoint" rotation="0 0 180"
                              position="0 -.2 0"></a-entity>
                </a-entity>


            </a-entity>
        </a-entity>
        <a-entity>
            <a-entity>
                <a-entity text="value: Color"  mixin="menuPlane"></a-entity>
                <a-entity widget="method: edit-color" obj-model="obj: url(/assets/models/button.obj)" material="color: #fce; opacity: .6" scale=".5 .5 .5" rotation="90 0 0"></a-entity>
                <a-sphere mixin="widgetMaterial" radius=".1" position="0 0.05 0" material="color: #f00;"></a-sphere>
                <a-sphere mixin="widgetMaterial" radius=".1" position="0.05 0 0" material="color: #040;"></a-sphere>
                <a-sphere mixin="widgetMaterial" radius=".1" position="-0.05 0 0"
                          material="color: #00f;"></a-sphere>
                <a-entity position="-7 0 4" rotation="0 45 0" scale="9 9 9" id="color-picker" color-picker
                          visible="false"></a-entity>
            </a-entity>
        </a-entity>

        <a-entity>
            <a-entity text="value: Remove"  mixin="menuPlane"></a-entity>
            <a-entity widget="method: remove"
                      obj-model="obj: url(/assets/models/button.obj)"
                      material="color: #fce; opacity: .6"
                      scale=".5 .5 .5" rotation="90 0 0"></a-entity>
            <a-entity>
                <a-box mixin="widgetMaterial" width=".2" height="0.05" depth="0.05" color="#f55"></a-box>
            </a-entity>

        </a-entity>
        <!--<a-entity>
            <a-entity text="value: Light"  mixin="menuPlane"></a-entity>
            <a-entity widget="method: add-light" obj-model="obj: url(/assets/models/button.obj)" material="color: #fce; opacity: .6" scale=".5 .5 .5" rotation="90 0 0"></a-entity>
            <a-entity>
                <a-cylinder mixin="widgetMaterial" radius=".01" height="0.05" color="#fff"></a-cylinder>
            </a-entity>
        </a-entity>-->


        <a-entity>
            <a-entity text="value: Align" mixin="menuPlane"></a-entity>
            <a-entity widget="method: align" obj-model="obj: url(/assets/models/button.obj)" material="color: #fce; opacity: .6" scale=".5 .5 .5" rotation="90 0 0"></a-entity>
            <a-entity>
                <a-entity rotation="30 30 0">
                    <a-cylinder radius=".01" height="0.3" mixin="widgetMaterial"></a-cylinder>
                    <a-entity template="src: #widgetPoint" position="0 .2 0"></a-entity>
                    <a-entity template="src: #widgetPoint" rotation="0 0 180" position="0 -.2 0"></a-entity>

                </a-entity>
                <a-entity rotation="30 30 90">
                    <a-cylinder radius=".01" height="0.3" mixin="widgetMaterial"></a-cylinder>
                    <a-entity template="src: #widgetPoint" position="0 .2 0" opacity="0.5"></a-entity>
                    <a-entity template="src: #widgetPoint" rotation="0 0 180"
                              position="0 -.2 0"></a-entity>
                </a-entity>
                <a-entity rotation="120 30 0">
                    <a-cylinder radius=".01" height="0.3" color="#fff"></a-cylinder>
                    <a-entity template="src: #widgetPoint" position="0 .2 0"></a-entity>
                    <a-entity template="src: #widgetPoint" rotation="0 0 180"
                              position="0 -.2 0"></a-entity>
                </a-entity>


            </a-entity>
        </a-entity>
        <a-entity>
            <a-entity text="value: Copy" mixin="menuPlane"></a-entity>
            <a-entity widget="method: copy"
                      obj-model="obj: url(/assets/models/button.obj)"
                      material="color: #fce; opacity: .6"
                      scale=".5 .5 .5" rotation="90 0 0"></a-entity>

            <a-entity>
                <a-sphere mixin="widgetMaterial" radius=".1" position="-0.05 0 0"
                          material="color: #fff;"></a-sphere>
                <a-sphere mixin="widgetMaterial" radius=".1" position="0.05 0 0"
                          material="color: #fff; opacity: 0.5"></a-sphere>
            </a-entity>

        </a-entity>
        <a-entity>
            <a-entity text="value: Share" mixin="menuPlane"></a-entity>
            <a-entity widget="method: share"
                      obj-model="obj: url(/assets/models/button.obj)"
                      material="color: #fce; opacity: .6"
                      scale=".5 .5 .5" rotation="90 0 0"></a-entity>

        </a-entity>
        <a-entity>
            <a-entity text="value: Exit" mixin="menuPlane"></a-entity>
            <a-entity widget="method: exit"
                      obj-model="obj: url(/assets/models/button.obj)"
                      material="color: #fce; opacity: .6"
                      scale=".5 .5 .5" rotation="90 0 0"></a-entity>

        </a-entity>


    </a-entity>
</a-entity>